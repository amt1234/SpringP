package com.bridgeit.programs;

public class Point {

	private int x;
	private int y;

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	 public Point() {
		 
		System.out.println("Point "+getX()+" "+getY());
	}
	@Override
	public String toString() {
		System.out.println("point class");
		return "Point [x=" + x + ", y=" + y + "]";
	}
	

}
